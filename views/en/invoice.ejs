<body
    style="width:100%;height:100%;background:#e6e6e6;margin: 0;padding: 0 10px;box-sizing: border-box; text-align: left; font-weight: 390;">
    <table cellspacing="0" cellpadding="0" width="100%"
        style="background-color: #fff;padding: 0;border-collapse:collapse;margin: 50px auto;max-width: 500px;width: 100%;"
        border="0">
        <tbody>
            <tr>
                <td style="padding: 30px 30px 15px 30px;">
                    <table style="width: 100%;">
                        <tbody>
                            <tr style=" text-align: center;">
                                <td><img src="https://betazone1.promaticstechnologies.com/eurobose-rest-apis/public/imgs/euroboose-logo.png" alt="Dreeshah"
                                        style="width: 100%; height: 100%; max-width: 150px; object-fit: contain;" />
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td style="padding:0 30px;">
                    <table class="table" style="width: 100%; box-shadow: 10px 10px #fff; border: none;">
                        <tbody>
                            <tr>
                                <td style="border: none; font-size: 28px; font-weight: 600;">
                                    Invoice List
                                </td>
                                <td style="border: none; font-size: 17px; font-weight: 500;    text-align: end; vertical-align: middle;"><b>Date:</b> 20 January,2024</td>
                                <td style="border: none; font-size: 17px; font-weight: 500;    text-align: end; vertical-align: middle;"><b>Time:</b>02:15:51 PM</td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td style="padding: 10px 30px 30px 30px;">
                    <table class="table" style="width: 100%; box-shadow: 10px 10px #fff;">
                        <thead>
                            <tr>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">id</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Client Name</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Email</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Phone Number</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Registration
                                </th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Registration
                                </th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Brand Name
                                </th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Model</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Total</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Paid Amount
                                </th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Balance
                                </th>


                            </tr>
                        </thead>
                        <tbody>
                          <% for (var i = 0; i < data?.length; i++) { %>
                          <tr>
                            <td style="height: 30px; padding: 10px;"><%=i+1%></td>
                          <td style="height: 30px; padding: 10px;"><%= data[i]?.clientId?.firstName %> <%= data[i]?.clientId?.lastName %></td>
                          <td style="height: 30px; padding: 10px;"><%= data[i]?.clientId??.email %></td>
                          <td style="height: 30px; padding: 10px;"><%= data[i]?.clientId?.phoneNumber %></td>
                          <td style="height: 30px; padding: 10px;"><%= data[i]?.quotation_id?.regNumber %></td>
                          <td style="height: 30px; padding: 10px;"><%= data[i]?.quotation_id?.model %></td>
                          <td style="height: 30px; padding: 10px;"><%= data[i]?.quantity*data[i].price %></td>
                          <td style="height: 30px; padding: 10px;"><%= data[i]?.paidAmount %></td>
                          <td style="height: 30px; padding: 10px;"><%= data[i]?.balance %></td>

                          </tr>
                          <% } %>
                          </tbody>
                          
                    </table>
                </td>
            </tr>
        </tbody>
    </table>
</body>
