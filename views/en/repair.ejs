<body
    style="width:100%;height:100%;background:#e6e6e6;margin: 0;padding: 0 10px;box-sizing: border-box; text-align: left; font-weight: 390;">
    <table cellspacing="0" cellpadding="0" width="100%"
        style="background-color: #fff;padding: 0;border-collapse:collapse;margin: 50px auto;max-width: 800px;width: 100%;"
        border="0">
        <tbody>
            <tr>
                <td style="padding: 30px 30px 15px 30px;">
                    <table style="width: 100%;">
                        <tbody>
                            <tr style=" text-align: center;">
                                <td><img src="https://betazone1.promaticstechnologies.com/eurobose-rest-apis/public/imgs/euroboose-logo.png" alt="Dreeshah"
                                        style="width: 100%; height: 100%; max-width: 150px; object-fit: contain;" />
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td style="padding:0 30px;">
                    <table class="table" style="width: 100%; box-shadow: 10px 10px #fff; border: none;">
                        <tbody>
                            <tr>
                                <td style="border: none; font-size: 28px; font-weight: 600;">
                                  Repair List
                                </td>
                               <td style="border: none; font-size: 17px; font-weight: 500; text-align: end; vertical-align: middle;"
                                    id="dateField"><b>Date:</b> </td>
                                <td style="border: none; font-size: 17px; font-weight: 500; text-align: end; vertical-align: middle;"
                                    id="timeField"><b>Time:</b></td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td style="padding: 10px 30px 30px 30px;">
                    <table class="table" style="width: 100%; box-shadow: 10px 10px #fff;">
                        <thead>
                            <tr>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">ID</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Client Name</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Car</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Model</th>
                                <th scope="col" style="white-space: nowrap; border: 1px solid #000;">Registration Number
                                </th>


                            </tr>
                        </thead>
                        <tbody>
                      <%
data.forEach(function(item, index) {
%>
    <tr>
        <td style="white-space: nowrap; border: 1px solid #000;"><%= index + 1 %></td>
        <td style="white-space: nowrap; border: 1px solid #000;"><%= item.client_id ? item.client_id.firstName + ' ' + item.client_id.lastName : '' %></td>
        <td style="white-space: nowrap; border: 1px solid #000;"><%= item.regNumber %></td>
        <td style="white-space: nowrap; border: 1px solid #000;"><%= item.type_of_car ? item.type_of_car.main_title : '' %></td>
        <td style="white-space: nowrap; border: 1px solid #000;"><%= item.model %></td>
    </tr>
<%
});
%>
                            
                        </tbody>
                    </table>
                </td>
            </tr>
        </tbody>
    </table>
</body>
 <script>
        // JavaScript to set current date and time
        var currentDate = new Date().toLocaleDateString();
        var currentTime = new Date().toLocaleTimeString();

        document.getElementById("dateField").innerHTML = "<b>Date:</b> " + currentDate;
        document.getElementById("timeField").innerHTML = "<b>Time:</b> " + currentTime;
    </script>
