  <body
    style="
      width: 100%;
      height: 100%;
      background: #e6e6e6;
      margin: 0;
      padding: 0 10px;
      box-sizing: border-box;
      text-align: left;
      font-weight: 390;
    "
  >
    <table
      cellspacing="0"
      cellpadding="0"
      width="100%"
      style="
        background-color: #fff;
        padding: 0;
        border-collapse: collapse;
        margin: 50px auto;
        max-width: 600px;
        width: 100%;
      "
      border="0"
    >
      <tbody>
        <tr>
          <td style="padding: 30px 30px 15px 30px">
            <table style="width: 100%">
              <tbody>
                <tr style="text-align: center">
                  <td>
                    <img
                      src="https://eu2.contabostorage.com/8c1e3125e2014e96afa60e0de6388c29:eurobase-media/main-logo.png"
                      alt="Dreeshah"
                      style="
                        width: 100%;
                        height: 100%;
                        max-width: 150px;
                        object-fit: contain;
                      "
                    />
                  </td>
                </tr>
              </tbody>
            </table>
          </td>
        </tr>
        <tr>
          <td style="padding: 30px">
            <table style="width: 100%; box-shadow: 10px 10px #fff">
              <tbody>
                <tr>
                  <td
                    style="
                      color: #18364a;
                      font-size: 18px;
                      line-height: 25px;
                      font-weight: 600;
                    "
                  >
                    Dear <%=name%>
                  </td>
                </tr>
                <tr>
                  <td
                    style="
                      font-size: 16px;
                      line-height: 28px;
                      color: #000;
                      padding-bottom: 10px;
                      padding-top: 15px;
                      font-weight: 400;
                    "
                  >
                    We are adding you as a Technician for garage. This is your
                    credentials for login :-
                  </td>
                </tr>
                <tr>
                  <td style="padding: 10px 0"><b>Email id: </b><%=email%></td>
                </tr>
                <tr>
                  <td style="padding-bottom: 10px">
                    <b>Password: </b><%=password%>
                  </td>
                </tr>
                <tr>
                  <td style="padding-bottom: 26px">
                    <a href="<%= link %>">Website Link </a>
                  </td>
                </tr>
                <tr>
                  <td>We are also giving you access to these user features.</td>
                </tr>
                <tr>
                  <td>
                    <ul>
                      <% if (userHasAccessToQuote) { %>
                      <li style="padding-bottom: 10px">Quote</li>
                      <% } %> <% if (userHasAccessToCalendar) { %>
                      <li style="padding-bottom: 10px">Access the calendar</li>
                      <% } %> <% if (userHasAccessToWorkshopSchedule) { %>
                      <li style="padding-bottom: 10px">
                        Access the workshop schedule
                      </li>
                      <% } %> <% if (userHasAccessToInvoices) { %>
                      <li style="padding-bottom: 10px">Make invoices</li>
                      <% } %> <% if (userHasAccessToChangePrices) { %>
                      <li style="padding-bottom: 10px">Change prices</li>
                      <% } %> <% if (userHasAccessToCreateCustomers) { %>
                      <li style="padding-bottom: 10px">Create customers</li>
                      <% } %> <% if (userHasAccessToCreateRepairOrder) { %>
                      <li style="padding-bottom: 10px">Create repair order</li>
                      <% } %>
                    </ul>
                  </td>
                </tr>
                <tr>
                  <td style="padding: 20px 0 5px 0"><b>Thanks & Regards</b></td>
                </tr>
                <tr>
                  <td><b>Eurobose Dsp Tools</b></td>
                </tr>
              </tbody>
            </table>
          </td>
        </tr>
      </tbody>
    </table>
  </body>
